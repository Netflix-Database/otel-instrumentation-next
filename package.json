{
  "name": "@netflix-database/otel-instrumentation-next",
  "version": "0.0.4",
  "description": "OpenTelemetry instrumentation helpers for Next.js apps (Node runtime)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./logger": {
      "import": "./lib/logger.js",
      "require": "./lib/logger.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/Netflix-Database"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepare": "npm run build",
    "clean": "rimraf lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Netflix-Database/otel-instrumentation-next.git"
  },
  "keywords": [
    "opentelemetry",
    "nextjs",
    "instrumentation",
    "otel"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "pino-pretty": "^13.1.3",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@opentelemetry/api-logs": "^0.208.0",
    "@opentelemetry/exporter-logs-otlp-grpc": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.208.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/instrumentation-http": "^0.209.0",
    "@opentelemetry/instrumentation-pino": "^0.55.1",
    "@opentelemetry/instrumentation-undici": "^0.19.0",
    "@opentelemetry/sdk-logs": "^0.208.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "pino": "^10.1.0"
  }
}
